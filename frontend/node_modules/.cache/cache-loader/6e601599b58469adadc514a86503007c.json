 cfg.sourceNode.getModel();\r\n      const shape = group.addShape('path', {\r\n        attrs: {\r\n\r\n          path,\r\n          stroke:   '#1890FF',\r\n          endArrow: false,\r\n          ...cfg.style,\r\n          ...edgeStyle,\r\n        },\r\n        name:      'hvh-edge',\r\n        draggable: true,\r\n      });\r\n\r\n      const { note } = cfg.targetMode.getModel();\r\n\r\n      if (note) {\r\n        const label = group.addShape('text', {\r\n          attrs: {\r\n            x:          horizontalEndPoint.x + xOffset + 10, // 居中\r\n            y:          endPoint.y - 30,\r\n            autoRotate: true,\r\n            text:       note || '',\r\n            fill:       '#333',\r\n            // fill:       '#fff',\r\n            stroke:     '#fff',\r\n            fontSize:   16,\r\n          },\r\n          zIndex: 10,\r\n        });\r\n\r\n        group.sort();\r\n        label.toFront();\r\n      }\r\n\r\n      return shape;\r\n    },\r\n    ...cfg,\r\n  });\r\n};\r\n"],"mappings":"AAEA,gBAAe,CAACA,EAAE,EAAEC,GAAG,KAAK;EAC1BD,EAAE,CAACE,YAAY,CAAC,UAAU,EAAE;IAC1BC,IAAIA,CAACF,GAAG,EAAEG,KAAK,EAAE;MACf,MAAMC,OAAO,GAAG,EAAE;MAClB,MAAM;QAAEC,UAAU;QAAEC;MAAS,CAAC,GAAGN,GAAG;MACpC,MAAMO,KAAK,GAAGD,QAAQ,CAACE,CAAC,GAAGH,UAAU,CAACG,CAAC;MAEvC,MAAMC,kBAAkB,GAAG;QACzBC,CAAC,EAAEL,UAAU,CAACK,CAAC;QACfF,CAAC,EAAEF,QAAQ,CAACE;MACd,CAAC;MACD,MAAMG,IAAI,GAAGJ,KAAK,KAAK,CAAC,GAAG,CACzB,CAAC,GAAG,EAAEF,UAAU,CAACK,CAAC,EAAEL,UAAU,CAACG,CAAC,CAAC,EACjC,CAAC,GAAG,EAAEF,QAAQ,CAACI,CAAC,EAAEJ,QAAQ,CAACE,CAAC,CAAC,CAC9B,GAAG,CACF,CAAC,GAAG,EAAEH,UAAU,CAACK,CAAC,EAAEL,UAAU,CAACG,CAAC,CAAC,EACjC,CAAC,GAAG,EAAEH,UAAU,CAACK,CAAC,GAAGN,OAAO,EAAEC,UAAU,CAACG,CAAC,CAAC,EAC3C,CAAC,GAAG,EAAEC,kBAAkB,CAACC,CAAC,GAAGN,OAAO,EAAEE,QAAQ,CAACE,CAAC,GAAGH,UAAU,CAACG,CAAC,GAAGC,kBAAkB,CAACD,CAAC,GAAG,EAAE,GAAGC,kBAAkB,CAACD,CAAC,GAAG,EAAE,CAAC,EACxH,CAAC,GAAG,EAAEC,kBAAkB,CAACC,CAAC,GAAGN,OAAO,EAAEK,kBAAkB,CAACD,CAAC,EAAEC,kBAAkB,CAACC,CAAC,GAAGN,OAAO,GAAG,EAAE,EAAEK,kBAAkB,CAACD,CAAC,CAA